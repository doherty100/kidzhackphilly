ndx</field><value name=\"VALUE\"><block type=\"mouse_x_get\" id=\"IAxV4^fh@p;KetNpXKj!\"></block></value><next><block type=\"variables_set\" id=\"QbeX`|1lqOJT{kc`lUTM\"><field name=\"VAR\" id=\"0G)_3zey+gy0dB?MfuS+\" variabletype=\"\">baseballendy</field><value name=\"VALUE\"><block type=\"mouse_y_get\" id=\",!^iFi*E2$@|nS/q8oyB\"></block></value></block></next></block></next></block></next></block></statement></block></xml>","code":"var baseballnowx, alienx, baseballnowy, alieny, baseballendx, baseballendy, lerppct;\n\n\napp.onStart(function() {\n  ctx.background = '#039BE5';\n  ctx.color('#388E3C');\n  ctx.moveTo(400, 500);\n  ctx.ellipse(800, 200);ctx.moveTo(700, 100);\n  ctx.stamp('sun-smiling', 100, 0);text.value = 'Click where you want the alien to toss the baseball';\n  text.moveTo(400, 580);\n  alienx = 100;\n  alieny = 400;\n  ctx.moveTo(alienx, alieny);\n  ctx.stamp('alien', 100, 0);baseball.image = 'baseball';\n  baseball.setScale(25);\n  baseballnowx = alienx;\n  baseballnowy = alieny;\n  baseballendx = 800;\n  baseballendy = alieny;\n  lerppct = 0;\n  time.every(1, 'milliseconds', function () {\n    if (lerppct > 100) {\n      lerppct = 0;\n    }\n    baseball.moveTo(baseballnowx, baseballnowy);\n    baseballnowx = math.lerp(alienx, baseballendx, lerppct);\n    baseballnowy = math.lerp(alieny, baseballendy, lerppct);\n    lerppct += 1;\n  });\n\n});\n\nmouse.onDown(function() {\n  baseballnowx = alienx;\n  baseballnowy = alieny;\n  baseballendx = mouse.x;\n  baseballendy = mouse.y;\n\n});\n","parts":[{"type":"sticker","id":"baseball","name":"Baseball"},{"type":"mouse","id":"mouse","name":"Mouse"},{"type":"text","id":"text","name":"Text"}],"profile":"default","name":"Remix: Baseball toss Phase 4"}