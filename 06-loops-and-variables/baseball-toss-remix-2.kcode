eballx;\n\n\napp.onStart(function() {\n  ctx.background = '#039BE5';\n  ctx.color('#388E3C');\n  ctx.moveTo(400, 500);\n  ctx.ellipse(800, 200);ctx.moveTo(700, 100);\n  ctx.stamp('sun-smiling', 100, 0);ctx.moveTo(100, 400);\n  ctx.stamp('alien', 100, 0);baseballx = 200;\n  sticker.image = 'baseball';\n  sticker.setScale(25);\n  time.every(1, 'milliseconds', function () {\n    sticker.moveTo(baseballx, 400);\n    baseballx = baseballx + 1;\n    if (baseballx > 850) {\n      baseballx = 200;\n    }\n  });\n\n});\n","parts":[{"type":"sticker","id":"sticker","name":"Sticker"}],"profile":"default","name":"Remix: Remix: Baseball toss"}