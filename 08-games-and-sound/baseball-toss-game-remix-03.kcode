xt></block></statement></block></xml>","code":"var targetSpeedY, crosshairX, crosshairY, targetY, throwInProgress, alienX, alienY, targetX, baseballX, baseballY, score;\n\n\napp.onStart(function() {\n  ctx.background = '#039BE5';\n  text.moveTo(300, 40);\n  text.setScale(300);\n  alienX = 70;\n  alienY = 400;\n  targetX = 720;\n  targetY = 400;\n  baseballX = alienX;\n  baseballY = alienY;\n  score = 0;\n\n});\n\napp.onStart(function() {\n  time.every(1, 'frames', function () {\n    ctx.reset();\n    ctx.color('#388E3C');\n    ctx.moveTo(400, 500);\n    ctx.ellipse(800, 200);ctx.moveTo(70, 100);\n    ctx.stamp('sun-smiling', 100, 0);ctx.moveTo(alienX, alienY);\n    ctx.stamp('alien', 100, 0);ctx.moveTo(targetX, targetY);\n    ctx.color('#FF0000');\n    ctx.circle(80);\n    ctx.color('#FFFFFF');\n    ctx.circle(50);\n    ctx.color('#FF0000');\n    ctx.circle(30);\n    ctx.color('#FFFFFF');\n    ctx.circle(10);\n    ctx.moveTo(baseballX, baseballY);\n    ctx.stamp('baseball', 25, 0);text.value = score;\n    ctx.moveTo(crosshairX, crosshairY);\n    ctx.stroke('#000', 5);\n    ctx.line(0, -20);\n    ctx.line(0, 20);\n    ctx.line(-20, 0);\n    ctx.line(20, 0);\n  });\n\n});\n\napp.onStart(function() {\n  targetSpeedY = -5;\n  time.every(1, 'frames', function () {\n    targetY += targetSpeedY;\n    if (targetY > 520) {\n      targetSpeedY = math.random(-10, -1);\n      speaker.play('elec_twip');\n    }\n    if (targetY < 80) {\n      targetSpeedY = math.random(1, 10);\n      speaker.play('elec_twip');\n    }\n  });\n\n});\n\napp.onStart(function() {\n  crosshairX = 400;\n  crosshairY = 300;\n  throwInProgress = 0;\n\n});\n\nmouse.onMove(function() {\n  if (!throwInProgress) {\n    crosshairX = mouse.x;\n    crosshairY = mouse.y;\n  }\n\n});\n\nmouse.onUp(function() {\n  crosshairX = mouse.x;\n  crosshairY = mouse.y;\n  throwInProgress = 1;\n  speaker.play('perc_swash');\n\n});\n","parts":[{"type":"text","id":"text","name":"Text"},{"type":"speaker","id":"speaker","name":"